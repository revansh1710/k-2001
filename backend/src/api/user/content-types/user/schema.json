{
  "collectionName": "user",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "users-permissions": {
      "advanced": {
        "unique_email": true
      }
    }
  },
  "attributes": {
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true,
      "private": true
    },
    "fullName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    }
  }
}
